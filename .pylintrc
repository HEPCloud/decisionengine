# .pylintrc to control pylint defaults

[TYPECHECK]
# List of decorators that produce context managers
contextmanager-decorators=contextlib.contextmanager,tf_contextlib.contextmanager

[MESSAGES CONTROL]
# Enable/disable message, report, category
# enable only errors
#  [KJK]:  Once https://github.com/PyCQA/pylint/issues/6811 is resolved, the following set of statements should be used
# errors-only=yes
# disable=no-member
disable=W,C,R,no-member
